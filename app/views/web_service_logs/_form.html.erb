<%= simple_form_for(@web_service_log) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :service %>
    <%= f.input :query %>
    <%= f.input :response_code %>
    <%= f.input :response_time %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
