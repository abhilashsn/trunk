<div id="login_main">
<h1>Listing EOB Errors</h1>
<div class="light_info">
  <%= link_to 'New EOB Error', :action => 'new' %>
</div>

 <%=will_paginate @eob_errors%>
<table  style="width: 938px">
  <tr>
  <% for column in EobError.content_columns %>
    <th><%= column.human_name.titleize %></th>

  <% end %>
  <th style="width: 100px"> &nbsp;</th>
  </tr>

<% for eob_error in @eob_errors %>
  <tr>
  <% for column in EobError.content_columns %>
    <td><%=h eob_error.send(column.name) %></td>
  <% end %>
    <td><%= link_to image_tag("report_edit.png", :alt => "Edit"), :action => 'edit', :id => eob_error %></td>
    <td><%= link_to image_tag("delete.png", :alt => "Destroy"), {:action => 'error_delete', :id => eob_error}, :confirm => "Are you sure?" %></td>
    </tr>
<% end %>
</table>

<br />
</div>
