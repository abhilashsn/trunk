<div id="login_main">
    <h4>Upload files for parsing </h4>
    <%= form_tag({:action => 'upload_provider_csv' }, :multipart => true) do %>
    <p class="black_text">
        Local File : <%= file_field "upload", "file" %>
    </p>
    <p>
        <%= submit_tag "Import", :class => 'button' %>
    </p>
    <% end %>
</div>
